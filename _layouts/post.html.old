---
layout: default
---

<script src="https://data-1.science/simple-jekyll-search.min.js" type="text/javascript"></script>

<script>
SimpleJekyllSearch({
  searchInput: document.getElementById('search-input'),
  resultsContainer: document.getElementById('results-container'),
  json: 'https://data-1.science/search.json'
})
</script>

<script>
$(document).ready(function(){
	$("#search-container a").click(function(){
		var value = $(this).html();
		var input = $('#search-input');
		input.val(value);
	});
});
</script>





<article class="post" itemscope itemtype="http://schema.org/BlogPosting">

  <header class="post-header">
    <h1 class="post-title" itemprop="name headline">{{ page.title | escape }}</h1>
    <p class="post-meta">
      <time datetime="{{ page.date | date_to_xmlschema }}" itemprop="datePublished">
        {% assign date_format = site.minima.date_format | default: "%b %-d, %Y" %}
        {{ page.date | date: date_format }}
      </time>
      {% if page.author %}
        â€¢ <span itemprop="author" itemscope itemtype="http://schema.org/Person"><span itemprop="name">{{ page.author }}</span></span>
      {% endif %}
    </p>
   </header>

  <div class="post-content" itemprop="articleBody">
    {{ content }}
  </div>
<p></p>
<div class="relatedPosts">

	<h4><b>Tags</b></h4>

	<ul class="tags">

	{% for tag in page.tags %}
    		
		<li><a href="#" class="tag">{{ tag }}</a></li>


	
		
	{% endfor %}
	</ul>
	
	<div id="search-container">
		<input type="text" id="search-input" placeholder="search...">
	</div>

	<p><ul id="results-container"></ul></p>

</div>

</article>
